package com.systex.main;

public class MultiTable {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for (int i = 1; i <= 9; i++) {
			for (int j = 1; j <= 9; j++) {
				System.out.print(i + "*" + j + "=" + ((i * j < 10) ? " " : "") + " ");
			}
			System.out.println();
		}
	}

}
